<!DOCTYPE html>
<html>
	<head>
		<link rel="stylesheet" href="main.css">
		<title>Etsy</title>
	
	</head>
		<body>
			<div class="main_box">
			<div class="header">
				<li> this is the header </li>
				<div class="category_list">
					<li> this is the category list </li>
					<li> first category </li>
					<li> second category </li>
				</div>
			</div>
		

			<div class="side_bar">
				<ul>
					<li> this is the nav bar</li>

				</ul>
			</div>

			<div class="listings">	
				<% listings.each do |listing| %>
					<div class="item">
						<div class="pic">
							<img class="pic_img" src="<%= listing["Images"][0]["url_170x135"]%>">
					    </div>
					    <div class="item_footer">
					    	<div class="price">
					    		<%= listing["price"] %>
					    	</div>
					    </div>
					</div>	
				<% end %>
			</div>
		</div>
		</body>
</html>
